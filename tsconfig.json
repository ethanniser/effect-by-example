{
  "extends": "astro/tsconfigs/strictest",
  "include": [".astro/types.d.ts", "**/*", "node"],
  "exclude": ["dist"],
  "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "baseUrl": ".",
    "allowJs": true,
    "lib": ["DOM"],
    "paths": {
      "@/*": ["src/*"]
    }
  }
}
